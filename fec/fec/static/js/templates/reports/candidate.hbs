<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">{{document_description}}
    {{#if is_amended}}
      (This report has been amended)
    {{/if}}
    </h4>
    {{#if amendment_version_description}}
    <i class="icon-document--inline--left"></i>
    {{{amendment_version_description}}} |
    {{/if}}
    {{{amendment_version}}}
  </div>
  <table>
    {{#panelRow "Filer"}}
      <a href="{{basePath}}/committee/{{committee_id}}">{{ committee_name }}</a>
    {{/panelRow}}
    {{#panelRow "FEC file number"}}
      {{ fec_file_id }}
    {{/panelRow}}
    {{#panelRow "Report coverage"}}
      {{datetime coverage_start_date format="pretty"}} to {{datetime coverage_end_date format="pretty"}}
    {{/panelRow}}
    {{#panelRow "Receipt date"}}
      {{datetime receipt_date format="pretty"}}
    {{/panelRow}}
    {{#panelRow "Filing method"}}
      {{ decodeMeans means_filed}}
    {{/panelRow}}
    {{#panelRow "Read report"}}
      {{#if html_url}}
        <a target="_blank" href="{{ html_url }}">Webpage</a> |
      {{/if}}
      {{#if csv_url}}
      <a target="_blank" href="{{ csv_url }}">.csv</a> |
      {{/if}}
      {{#if pdf_url }}
      <a target="_blank" href="{{ pdf_url }}">.pdf</a>
      {{/if}}
      {{#if fec_url}}
      | <a target="_blank" href="{{ fec_url }}">.fec</a>
      {{/if}}
    {{/panelRow}}
  </table>
</div>
<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">Raising</h4>
  </div>
  <table>
    {{#panelRow "Total receipts"}}
      {{{currency total_receipts_period}}}
    {{/panelRow}}
    {{#panelRow "Individual contributions"}}
      {{{currency total_individual_contributions_period}}}
    {{/panelRow}}
    {{#panelRow "Itemized individual contributions"}}
      {{{currency individual_itemized_contributions_period}}}
    {{/panelRow}}
    {{#panelRow "Unitemized individual contributions"}}
      {{{currency individual_unitemized_contributions_period}}}
    {{/panelRow}}
    {{#panelRow "Contributions from PACs"}}
      {{{currency political_party_committee_contributions_period}}}
    {{/panelRow}}
    {{#panelRow "Total contribution refunds"}}
      {{{currency total_contribution_refunds_period}}}
    {{/panelRow}}
    {{#if net_contributions_period}}
      {{#panelRow "Net contributions period"}}
        {{{currency net_contributions_period}}}
      {{/panelRow}}
    {{else}}
      {{#panelRow "Net contributions cycle to date"}}
        {{{currency net_contributions_cycle_to_date}}}
      {{/panelRow}}
    {{/if}}
    {{#if federal_funds_period}}
      {{#panelRow "Presidential public funds accepted"}}
        {{{currency federal_funds_period}}}
      {{/panelRow}}
    {{/if}}
  </table>
</div>
<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">Spending</h4>
  </div>
  <table>
    {{#panelRow "Total disbursements"}}
      {{{currency total_disbursements_period}}}
    {{/panelRow}}
    {{#if net_operating_expenditures_period}}
      {{#panelRow "Net operating expenditures period"}}
        {{{currency net_operating_expenditures_period}}}
      {{/panelRow}}
    {{else}}
      {{#panelRow "Net operating expenditures cycle to date"}}
        {{{currency net_operating_expenditures_cycle_to_date}}}
      {{/panelRow}}
    {{/if}}
    {{#panelRow "Transfers to authorized committees"}}
      {{{currency transfers_to_other_authorized_committee_period}}}
    {{/panelRow}}
  </table>
</div>
<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">Cash on hand</h4>
  </div>
  <table>
    {{#panelRow "Beginning cash on hand"}}
      {{{currency cash_on_hand_beginning_period}}}
    {{/panelRow}}
    {{#panelRow "Ending cash on hand"}}
      {{{currency cash_on_hand_end_period}}}
    {{/panelRow}}
  </table>
</div>
<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">Debts</h4>
  </div>
  <table>
    {{#panelRow "Debts owed by committee"}}
      {{{currency debts_owed_by_committee}}}
    {{/panelRow}}
    {{#panelRow "Debts owed to committee"}}
      {{{currency debts_owed_to_committee}}}
    {{/panelRow}}
  </table>
</div>
<div class="panel__row">
  <div class="panel__heading">
    <h4 class="panel__title">Loans</h4>
  </div>
  <table>
    {{#panelRow "Total loans received"}}
      {{{currency total_loans_received_period}}}
    {{/panelRow}}
    {{#panelRow "Loan repayments made"}}
      {{{currency total_loan_repayments_made_period}}}
    {{/panelRow}}
  </table>
</div>
